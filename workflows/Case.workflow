    <rules>
        <fullName>CSEDownloadedDateUpdate</fullName>
        <actions>
            <name>CSEDownloadedDateUpdate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Where insurer has downloaded the file, this time stamps the latest date</description>
        <formula>AND(    ISPICKVAL(InsurerStatus__c, &quot;Downloaded&quot;),    RecordType.DeveloperName = &quot;Online_Claims_Notification&quot;     )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CSEInsurerEmailFieldUpdate</fullName>
        <actions>
            <name>CSEUpdateInsurerEmailField</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>If CAS team member updates Insurer Code field then the Insurer Email Field should be updated with email from Insurer</description>
        <formula>AND(RecordType.DeveloperName = &quot;Online_Claims_Notification&quot;,    Account.RecordType.DeveloperName = &quot;Business_OLCN&quot; , TEXT(InsurerCode__c) &lt;&gt; Null)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>CSEInsurerFieldUpdates</fullName>
        <actions>
            <name>CSEInsurerProcessDateDownloaded</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CSEInsurerProcessUpdateNew</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CSEInsurerUpdateMatchedBy</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CSEInsurerUpdateMatchedDate</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>CSEUpdateInsurerEmailField</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If CAS team member updates Insurer Code field then the Matched By and Matched Date field will be auto updated</description>
        <formula>AND(   RecordType.DeveloperName = &quot;Online_Claims_Notification&quot;,   TEXT(PRIORVALUE(InsurerCode__c)) &lt;&gt; TEXT(InsurerCode__c)  /*True = ISCHANGED - rules always fires on if insurer code changes*/ )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>CSEInsurerProcessStatusUpdate</fullName>
        <actions>
            <name>CSEInsurerProcessStatusUpdate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If Insurer Process Status is Downloaded and if one the fields mentioned in this workflow is updated then the Insurer Process Status changes to Updated.</description>
        <formula>AND(  OR(  ISCHANGED( FormFirstName__c  ),   ISCHANGED( FormLastName__c ) ,  ISCHANGED( FormEmail__c  ) ,  ISCHANGED( FormPhone__c  ),  ISCHANGED( FormSubmitter__c )  ,  ISCHANGED( RepType__c ) ,  ISCHANGED( RepTypeOther__c ) ,  ISCHANGED( FormRepFirstName__c ) ,  ISCHANGED( FormRepLastName__c  ) ,  ISCHANGED( RepEmail__c ) ,  ISCHANGED( RepPhone__c ) ,  ISCHANGED( AccidentDate__c ) ,  ISCHANGED( AccidentRole__c ) ,  ISCHANGED( AccidentRoleOther__c ) ,  ISCHANGED( AtFaultRegistrationNumber__c ) ,  ISCHANGED( DescriptionOfVehicle__c ) ,  ISCHANGED( DriverName__c ) ,  ISCHANGED( AccidentLocation__c ) ,  ISCHANGED( SendTo__c),  ISCHANGED( ContactId),  ISCHANGED( RepContact__c)  ),  TEXT( PRIORVALUE( InsurerStatus__c )) = &quot;Downloaded&quot;,  RecordType.DeveloperName = &quot;Online_Claims_Notification&quot; )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>CSETResetOutboundCallFlagFalse</fullName>
        <actions>
            <name>CSEUpdateCreateOutboundCallFlagFalse</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Where Insurers have changed over time and an outbound call has been created. Resets the flag. OLCN</description>
        <formula>AND(  ISCHANGED(MatchedDate__c),  CreateOutboundCallFlag__c = TRUE,  RecordType.DeveloperName = &quot;Online_Claims_Notification&quot; )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>CSEToUpdateCreateOutboundCallFlag</fullName>
        <active>false</active>
        <booleanFilter>1 AND 2</booleanFilter>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>In Progress</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.CreateOutboundCallFlag__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>GB to put description</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>CSEUpdateCreateOutboundCallFlag</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Case.MatchedDate__c</offsetFromField>
            <timeLength>21</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
