            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>IPM_Insurer_Review_Process__c.IPM_Sufficient_Security_Lodged_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>IPM_Insurer_Review_Process__c.Security_Level_Review__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>IPM_Update_Process_Outcome_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
