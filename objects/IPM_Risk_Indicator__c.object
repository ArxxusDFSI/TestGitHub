    <fields>
        <fullName>IPM_Risk_Rating_Colour__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=1, value(TEXT(IPM_Risk_Rating__c)) &lt;= 6) , &quot;Green&quot;, 
IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=7, value(TEXT(IPM_Risk_Rating__c)) &lt;= 15) , &quot;Yellow&quot;, 
IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=16, value(TEXT(IPM_Risk_Rating__c)) &lt;= 22) , &quot;Orange&quot;, 
IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=23, value(TEXT(IPM_Risk_Rating__c)) &lt;= 25) , &quot;Red&quot;,
&quot;&quot; 
) 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Risk Rating Colour</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IPM_Risk_Rating_Image__c</fullName>
        <description>Graphical interpretation of the Risk Rating values:
1-6 = Green Low Risk
7-15 = Yellow Medium Risk
16-22 = Orange High Risk
23-25 = Red Extreme Risk</description>
        <externalId>false</externalId>
        <formula>IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=1, value(TEXT(IPM_Risk_Rating__c)) &lt;= 6) , IMAGE(&quot;/servlet/servlet.FileDownload?file=0157F0000000xnM&quot;, &quot;Green&quot;),
IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=7, value(TEXT(IPM_Risk_Rating__c)) &lt;= 15) , IMAGE(&quot;/servlet/servlet.FileDownload?file=0157F0000000xnP&quot;, &quot;Yellow&quot;),
IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=16, value(TEXT(IPM_Risk_Rating__c)) &lt;= 22) , IMAGE(&quot;/servlet/servlet.FileDownload?file=0157F0000000xnN&quot;, &quot;Orange&quot;),
IF( AND( value(TEXT(IPM_Risk_Rating__c)) &gt;=23, value(TEXT(IPM_Risk_Rating__c)) &lt;= 25) , IMAGE(&quot;/servlet/servlet.FileDownload?file=0157F0000000xnO&quot;, &quot;Red&quot;),&quot;&quot;
)
)
)
)</formula>
        <label>Risk Rating Image</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
