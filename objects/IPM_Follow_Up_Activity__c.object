    <validationRules>
        <fullName>Date_Advised_must_be_populated</fullName>
        <active>true</active>
        <description>If the Follow Up Activity Type = Strategic Impact then the Date Advised must be populated.</description>
        <errorConditionFormula>TEXT( IPM_Type__c) = &quot;Strategic Impact&quot; &amp;&amp; TEXT(  IPM_Date_Advised__c) = &quot;&quot;</errorConditionFormula>
        <errorMessage>Date Advised must be selected for a Strategic Impact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_must_be_populated</fullName>
        <active>true</active>
        <description>If the Follow Up Activity Type = Monitoring then the Due Date must be populated.</description>
        <errorConditionFormula>TEXT( IPM_Type__c ) = &quot;Monitoring&quot; &amp;&amp; TEXT( IPM_Due_Date__c ) = &quot;&quot;</errorConditionFormula>
