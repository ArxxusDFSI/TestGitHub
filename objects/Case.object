    <validationRules>
        <fullName>DRS_CompleteDecision</fullName>
        <active>true</active>
        <description>When Case Status is updated to Decision, ensure that Decision Date and Outcome fields are complete.</description>
        <errorConditionFormula>IF( RecordType.DeveloperName = &quot;Merit_Review_Service&quot;,

IF(ISPICKVAL(Status, &quot;Decision&quot;), 
AND(
OR(
 ISBLANK( BriefSummaryOfOutcome__c ) ,
 ISBLANK( DecisionDate__c ) 
)
)
, False)
,False
)</errorConditionFormula>
        <errorMessage>Please ensure you have completed the fields &apos;Brief Summary of Outcome&apos; and &apos;Decision Date&apos; in the MRS Decision section before updating the Case to Decision.</errorMessage>
    </validationRules>
