            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Step 1 - Technical Manager QA</label>
        <name>IPM_Step_1_Technical_Manager_QA</name>
    </approvalStep>
    <description>Submits Change in Subsidiary Insurer Review Process for approval from QA</description>
    <emailTemplate>IPM_IPMG_Subsidiary_Review/IPM_Subsidiary_Approval_Review_Template</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>IPM_Insurer_Review_Process__c.RecordType</field>
            <operation>equals</operation>
            <value>Review of Subsidiaries</value>
        </criteriaItems>
        <criteriaItems>
            <field>IPM_Insurer_Review_Process__c.Subsidiary_Action__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
