        <actions>
            <name>OLCNCSEChangeOfContactAlertToInsurer</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Change of contact details alert to insurer</description>
        <formula>AND(OR( PRIORVALUE(OLCN_FormEmail__c  ) &lt;&gt; OLCN_FormEmail__c , PRIORVALUE(OLCN_FormPhone__c  ) &lt;&gt; OLCN_FormPhone__c,PRIORVALUE(OLCN_FormFirstName__c  ) &lt;&gt; OLCN_FormFirstName__c,PRIORVALUE(OLCN_FormLastName__c) &lt;&gt; OLCN_FormLastName__c), RecordType.DeveloperName = &quot;Online_Claims_Notification&quot; ,   ISPICKVAL(Origin, &quot;Web&quot;), NOT(ISBLANK( OLCN_InsurerEmail__c)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>OLCNCSEDownloadedDateUpdate</fullName>
        <actions>
            <name>OLCNCSEDownloadedDateUpdate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Where insurer has downloaded the file, this time stamps the latest date</description>
        <formula>AND(    ISPICKVAL(OLCN_InsurerStatus__c, &quot;Downloaded&quot;),    RecordType.DeveloperName = &quot;Online_Claims_Notification&quot;     )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>OLCNCSEInsurerEmailFieldUpdate</fullName>
        <actions>
            <name>OLCNCSEUpdateInsurerEmailField</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If CAS team member updates Insurer Code field then the Insurer Email Field should be updated with email from Insurer</description>
        <formula>AND(RecordType.DeveloperName = &quot;Online_Claims_Notification&quot;,    Account.RecordType.DeveloperName = &quot;Business_OLCN&quot; , TEXT(OLCN_InsurerCode__c) &lt;&gt; Null)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>OLCNCSEInsurerFieldUpdates</fullName>
        <actions>
            <name>OLCNCSECaseStatusUpdatePendAcceptance</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>OLCNCSEInsurerProcessDateDownloaded</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>OLCNCSEInsurerProcessUpdateNew</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>OLCNCSEInsurerUpdateMatchedBy</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>OLCNCSEInsurerUpdateMatchedDate</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>OLCNCSEUpdateInsurerEmailField</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If CAS team member updates Insurer Code field then the Matched By and Matched Date field will be auto updated</description>
        <formula>AND(   RecordType.DeveloperName = &quot;Online_Claims_Notification&quot;,   TEXT(PRIORVALUE(OLCN_InsurerCode__c)) &lt;&gt; TEXT(OLCN_InsurerCode__c)  /*True = ISCHANGED - rules always fires on if insurer code changes*/ )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>OLCNCSEInsurerProcessStatusUpdate</fullName>
        <actions>
            <name>OLCNCSEInsurerProcessStatusUpdate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If Insurer Process Status is Downloaded and if one the fields mentioned in this workflow is updated then the Insurer Process Status changes to Updated.</description>
        <formula>AND(  OR(  ISCHANGED( OLCN_FormFirstName__c  ),   ISCHANGED( OLCN_FormLastName__c ) ,  ISCHANGED( OLCN_FormEmail__c  ) ,  ISCHANGED( OLCN_FormPhone__c  ),  ISCHANGED( OLCN_FormSubmitter__c )  ,  ISCHANGED( OLCN_RepType__c ) ,  ISCHANGED( OLCN_RepTypeOther__c ) ,  ISCHANGED( OLCN_FormRepFirstName__c ) ,  ISCHANGED( OLCN_FormRepLastName__c  ) ,  ISCHANGED( OLCN_RepEmail__c ) ,  ISCHANGED( OLCN_RepPhone__c ) ,  ISCHANGED( OLCN_AccidentDate__c ) ,  ISCHANGED( OLCN_AccidentRole__c ) ,  ISCHANGED( OLCN_AccidentRoleOther__c ) ,  ISCHANGED( OLCN_AtFaultRegistrationNumber__c ) ,  ISCHANGED( OLCN_DescriptionOfVehicle__c ) ,  ISCHANGED( OLCN_DriverName__c ) ,  ISCHANGED( OLCN_AccidentLocation__c ) ,  ISCHANGED( OLCN_SendTo__c),  ISCHANGED( ContactId),  ISCHANGED( OLCN_RepContact__c)  ),  TEXT( PRIORVALUE( OLCN_InsurerStatus__c )) = &quot;Downloaded&quot;,  RecordType.DeveloperName = &quot;Online_Claims_Notification&quot; )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>OLCNCSETResetOutboundCallFlagFalse</fullName>
        <actions>
            <name>OLCNCSEUpdateCreateOutboundCallFlagFalse</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Where Insurers have changed over time and an outbound call has been created. Resets the flag. OLCN</description>
        <formula>AND(  ISCHANGED(OLCN_MatchedDate__c),  OLCN_CreateOutboundCallFlag__c = TRUE,  RecordType.DeveloperName = &quot;Online_Claims_Notification&quot; )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>OLCNCSEToUpdateCreateOutboundCallFlag</fullName>
        <active>false</active>
        <booleanFilter>1 AND 2</booleanFilter>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>In Progress</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.OLCN_CreateOutboundCallFlag__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>GB to put description</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>OLCNCSEUpdateCreateOutboundCallFlag</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Case.OLCN_MatchedDate__c</offsetFromField>
            <timeLength>21</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
</Workflow>
